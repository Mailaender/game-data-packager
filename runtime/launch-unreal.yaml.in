---
binary_only: true
required_files:
  - System/Core.so
  - System/Core.u
  - System/UnrealLinux.bin
  - Maps/Entry.unr
base_directories:
  - /usr/lib/unreal-classic
  - /usr/lib/unreal
  - /$assets/unreal
  - /$assets/unreal-ut99-shared
working_directory: $${XDG_DATA_HOME}/unreal/System
library_path: ["."]
argv: [./UnrealLinux.bin]
symlink_into_dot_directory:
  - Help
  - Maps
  - Meshes
  - Music
  - Sounds
  - System
  - Textures
copy_into_dot_directory:
  - System/*.ini
...
