_game_data_packager()
{
    if [ $COMP_CWORD -eq 1 ]
    then
      local cur supported
      cur=${COMP_WORDS[COMP_CWORD]}
      supported=$(cd  /usr/share/games/game-data-packager/; ls -1 *.yaml |cut -d'.' -f1)
      COMPREPLY=( $( compgen -W "$supported" -- $cur ) )
    else
      COMPREPLY=()
    fi
}

complete -o default -F _game_data_packager game-data-packager
